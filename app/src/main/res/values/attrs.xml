<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ConfirmButton">
        <attr name="isActive" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="FennecEditText">
        <attr name="isEditable" format="boolean"/>
    </declare-styleable>



    <declare-styleable name="SectionSwitcher">
        <attr name="firstTitle" format="string"/>
        <attr name="secondTitle" format="string"/>
        <attr name="sectionIsActive" format="enum">
            <enum name="first" value="0"/>
            <enum name="second" value="1"/>
        </attr>
    </declare-styleable>

</resources>

